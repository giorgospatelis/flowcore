#!/usr/bin/env php
<?php
require __DIR__ . '/../vendor/autoload.php';

use FlowCore\FlowCore;
use Symfony\Component\Console\Application;
use FlowCore\Command\DispatchJobCommand;
use FlowCore\Command\StartWorkerCommand;

FlowCore::initialize();
$application = new Application('FlowCore', '0.0.1');
$application->add(new DispatchJobCommand());
$application->add(new StartWorkerCommand());
$application->run();
